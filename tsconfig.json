{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023"],
    "module": "node16",
    "typeRoots": [
      "./node_modules/@types/",
      "./src/http/@types",
      "./node_modules"
    ],
    "types": ["vite/client", "vitest/globals", "node"],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
